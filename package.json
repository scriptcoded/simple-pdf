    "@babel/core": "^7.11.4",
    "@types/jest": "^26.0.10",
    "babel-preset-airbnb": "^5.0.0",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.7.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.0",
